{
  "manifest_version": 2,

  "name": "elementZapper",
  "description": "Disappear at once!",
  "version": "1.0",
  "icons": {
	"128": "zap_128.png",
	"64": "zap_64.png",
	"32": "zap_32.png",
	"16": "zap_16.png"
  },
  "permissions": [
	"contextMenus", 
	"tabs"
  ],
  "background": {
    "scripts": [
		"eventPage.js"
	],
	"persistent": false
  },
  "content_scripts": [
	{
		"matches": [
			"http://*/*",
			"https://*/*"
		],
		"js": [
			"contentScript.js"
		]
	}
  ]
}